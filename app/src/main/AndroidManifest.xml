        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme">
        <activity android:name=".Token"></activity>
        <activity android:name=".forgotpassword" />
        <activity android:name=".IncomeTax" />
        <activity android:name=".Votersid" />
        <activity android:name=".DeathCertificate" />
        <activity android:name=".BirthCertficate" />
        <activity android:name=".optionspage" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <activity
            android:name=".Registration"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
    </application>

</manifest>